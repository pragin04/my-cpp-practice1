#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    if(a>b&&b>a)
    {
        cout<<a<<"is greater number";
    }
    
    else if(b>a&&b>c)
    {
        cout<<b<<"is greater number";
    }
    else
    {
        cout<<c<<"is greater number";
    }
    return 0;
}
