#include<iostream>
using namespace std;
int main()
{
    int mark;
    cin>>mark;
    if (mark>=50)
    {
        if(mark>=90)
        {
        cout<<mark<< "the topper student";
        }
        else
        {
            cout<<mark<<" the average student";
        }
    }
    else
    {
        cout<<mark<<": "<<"the poor student";
    }
return 0;
}
